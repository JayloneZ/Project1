<tr>
<td><%= @eatery.name %></td>
<td>|<td>
<td><%= @eatery.address %></td>
<td>|<td>
<td><%= @eatery.url %></td>
<% deals = @eatery.find_deals %>
<% if deals != nil %>
<p>Deals:</p>
<ul>
<% for deal in deals %>
<li><a href="/deals/<%= deal.id %>"><%= deal.name %></a></li>
<% end %>
</ul>
<% end %>

</tr>
